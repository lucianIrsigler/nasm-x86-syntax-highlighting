        {
            "prefix":"NASMforloop2",
            "body":[
                ";Asks user for input and loops to that number",
                "SECTION .data",
                "\tstart db \"Enter number:\",0",
                "\tformatin: db \"%d\",0",
                "\tformatout db \"%d\",10,0",
                "\tinteger1: times 4 db 0 ; 32-bits integer = 4 bytes\n",
                "SECTION .texte",
                "\tglobal _main ",
                "\textern _printf",
                "\textern _scanf\n",
                "_main:",
                "\tpush start",
                "\tcall _printf",
                "\tadd esp,4",
                "\tpush integer1",
                "\tpush formatin",
                "\tcall _scanf",
                "\tadd esp,8",
                "\tmov ebx, dword [integer1]",
                "\tmov ecx,1",
                "loopstart:",
                "\tcmp ecx,ebx",
                "\tjz end  ",
                "\tpush ecx",
                "loop:",
                "\tpush ecx",
                "\tpush formatout",
                "\tcall _printf",
                "\tadd esp,8",
                "\tpop ecx",
                "\tinc ecx",
                "\tjmp loopstart",
                "end:",
                "\tret"
            ]
        }
